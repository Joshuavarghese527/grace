    <div class="section bg-white">
      <div class="container">
          
  <div id="pages">
    <h2 class="block-title" data-sr='roll -45deg'>Upcoming Events</h2>
  <div class="row">
   <div class="col-md-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        Upcoming Events
      </div>
      <div class="panel-body">
        
        <% @event.each do |event| %>
          <div class="row">
            <div class="col-md-4">
              <%= image_tag event.photos[0].image.url(:thumb) if event.photos.length > 0 %>
            </div>    
            <div class="col-md-4">
              <h4><%= link_to event.title, events_path %></h4>
            </div>  
              <div class="col-md-4">
              <h4><%= event.date.strftime(" %A, %b  %d  %Y") %> <br><%= event.time.strftime("%l:%M %p")%></h4>  
              </div>
          </div>
          <hr>
        <% end %>

      </div>
    </div>
  </div>
</div>
